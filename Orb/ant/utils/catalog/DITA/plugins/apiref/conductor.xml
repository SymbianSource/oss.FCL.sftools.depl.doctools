<project>
    <!-- content targets -->
    <target name="apiref2xhtml">
      <!-- apiref2xhtml.xsl extends dita2xhtml via the plugin interface -->
      <antcall target="dita2xhtml"/>
      <copy todir="${output.dir}">
        <fileset dir="${basedir}${file.separator}demo${file.separator}apiref${file.separator}xsl">
          <include name="*.css"/>
        </fileset>
      </copy>
    </target>
</project>
