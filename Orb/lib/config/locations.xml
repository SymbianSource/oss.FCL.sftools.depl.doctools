<?xml version="1.0" encoding="iso-8859-1"?>
<build xmlns="http://symbian.com/xml/build"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://symbian.com/xml/build ../../schema/build/2_0.xsd">
